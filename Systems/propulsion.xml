  <fcs_function name="propulsion/engine/thrust-lbs">
   <function>
    <product>
     <property>hydro/environment/rho-slug_ft3</property>
     <value>0.000277777</value> <!-- 1/60^2 -->
     <property>propulsion/engine/engine-rpm</property>
     <property>propulsion/engine/engine-rpm</property>
     <!--value>6561</value-->        <!-- D^4 Monitor movement-->

     <!-- when propellor thrust is ahead (1),  59000 gives ~21.5 knots ahead;
          when propellor thrust is astern (-1), 2900 gives ~5 knots astern -->
     <table>
       <independentVar lookup="row">external_reactions/propeller/x</independentVar>
       <tableData>
          -1  2900
           1  59000
       </tableData>
     </table>

     <property>propulsion/engine/thrust-coefficient</property>
    </product>
   </function>
   <output>external_reactions/propeller/magnitude</output>
  </fcs_function>

