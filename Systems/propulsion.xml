 <channel name="simple steam engine model">

  <!-- Temporary system that just sets the engine mean effective pressure.

       The slide valves of the engine were most likely set to only admit full
       pressure steam for part of the stroke to utilize steam expansion.
       The effective mean pressure is then
         p = p_init * (1 + log R)/R - back pressure
       where R is the range of expansion, i.e.
       volume at cutoff / volume at end of stroke.

       The evidence seems to suggest that the cutoff point of this engine
       could be adjusted on the fly. Hence, there are (at least) two primary
       engine controls: the main steam thottle valve restricting the flow of
       steam to the engine and the control setting the cutoff point.

       Here:
         fcs/throttle-cmd-norm - The main steam throttle valve.
         fcs/mixture-cmd-norm  - The cutoff point.
  -->

  <fcs_function name="propulsion/engine/range-of-expansion">
   <function>
    <table>
     <independentVar lookup="row">fcs/mixture-cmd-norm</independentVar>
     <tableData>
       0.0    0.37
       1.0    1.00
     </tableData>
    </table>
   </function>
  </fcs_function>

  <fcs_function name="propulsion/engine/steam-mean-pressure-psi">
   <function>
    <product>
     <max>
      <value>0.0</value>
      <sum>
       <product>
        <property>propulsion/boiler/steam-absolute-pressure-psi</property>
        <quotient>
         <sum>
          <value>1.0</value>
          <ln>
           <property>propulsion/engine/range-of-expansion</property>
          </ln>
         </sum>
         <property>propulsion/engine/range-of-expansion</property>
        </quotient>
       </product>
       <value>-6.0</value> <!-- Condenser back-pressure. Guessed. -->
      </sum>
     </max>
     <table>
      <independentVar lookup="row">fcs/throttle-cmd-norm</independentVar>
      <tableData>
        0.0    0.0
        1.0    1.0
      </tableData>
     </table>
    </product>
   </function>
  </fcs_function>

  <fcs_function name="propulsion/engine/power-available-hp">
   <!-- IHP = P*L*A*N/33000 [Ripper:1909:HE]. Units differ slightly here. -->
   <function>
    <sum>
     <product>
      <value>0.90</value>          <!-- Efficiency IHP to BHP.
                                        ~0.9 according to [Ripper:1909:HE].
                                   -->
      <value>2.5252525e-06</value> <!-- To HP. -->
      <value>2.0</value>           <!-- 2 double acting cylinders. -->
      <property>propulsion/engine/steam-mean-pressure-psi</property>
      <sum>
       <!-- in^3 -->
       <value>27646.015</value>  <!-- Swept volume per cylinder out-stroke. -->
       <value>24725.905</value>  <!-- Swept volume per cylinder in-stroke.  -->
      </sum>
      <property>propulsion/engine/engine-rpm</property>
     </product>
     <product>
      <value>-0.035</value> <!-- Guess. Fricition. -->
      <property>propulsion/engine/engine-rpm</property>
      <property>propulsion/engine/engine-rpm</property>
     </product>
    </sum>
   </function>
  </fcs_function>

  <fcs_function name="propulsion/engine/power-difference-hp">
   <function>
    <difference>
     <property>propulsion/engine/power-available-hp</property>
     <property>propulsion/engine/power-required-hp</property>
    </difference>
   </function>
  </fcs_function>

  <integrator name="propulsion/engine/engine-rpm">
   <input>propulsion/engine/power-difference-hp</input>
   <c1>0.2</c1>
   <clipto>
    <min>0.0000001</min>
    <max>240.0</max>
   </clipto>
  </integrator>

  <!-- FIXME: This will start to lose precision after some hours. -->
  <integrator name="propulsion/engine/shaft-revolutions-norm">
   <input>propulsion/engine/engine-rpm</input>
   <c1>0.016666667</c1>
  </integrator>

  <fcs_function name="propulsion/engine/shaft-position-norm">
   <function>
    <difference>
     <property>propulsion/engine/shaft-revolutions-norm</property>
     <integer>
      <property>propulsion/engine/shaft-revolutions-norm</property>
     </integer>
    </difference>
   </function>
  </fcs_function>

 </channel>

