 <!-- Particle effects ======================================================-->

 <model>
  <name>Foreward boiler smoke</name>
  <path>Aircraft/USS_Olympia/Models/Effects/smoke.xml</path>
 <condition>
   <greater-than>
<!--  <property>fdm/jsbsim/hydro/vbx-fps</property> -->
    <property>fdm/jsbsim/propulsion/engine/steam-mean-pressure-psi</property>
    <value>15.0</value>
   </greater-than>
  </condition>
  <offsets>
   <!-- x/y/z == back/right/up -->
   <x-m> 35.900 </x-m>
   <y-m> 0.000 </y-m>
   <z-m> 16.800 </z-m>
  </offsets>
 </model>

 <model>
  <name>Aft boiler smoke</name>
  <path>Aircraft/USS_Olympia/Models/Effects/smoke.xml</path>
 <condition>
   <greater-than>
    <!-- <property>fdm/jsbsim/hydro/vbx-fps</property> -->
    <property>fdm/jsbsim/propulsion/engine/steam-mean-pressure-psi</property>
    <value>6.0</value>
   </greater-than>
  </condition>
  <offsets>
   <!-- x/y/z == back/right/up -->
   <x-m> 53.50 </x-m>
   <y-m> 0.00 </y-m>
   <z-m> 16.800 </z-m>
  </offsets>
 </model>


 <!-- Bow Wake ==============================================================-->
<model>
  <name>Bow Splash Port</name>
  <overlay>
   <particlesystem>
  <condition>
   <greater-than>
    <property>fdm/jsbsim/hydro/vbx-fps</property>
    <value>12.0</value>
   </greater-than>
  </condition>
    <shooter>
     <phi-min-deg>-45.0</phi-min-deg>
     <phi-max-deg>-50.0</phi-max-deg>
    </shooter>
   </particlesystem>
  </overlay>
  <path>Aircraft/USS_Olympia/Models/Effects/splash.xml</path>
  <offsets>
   <!-- x/y/z == back/right/up -->
   <x-m>  -0.10 </x-m>
   <y-m>  0.00 </y-m>
   <z-m>  0.00 </z-m>
  </offsets>
 </model> 

<model>
  <name>Bow Wake Port</name>
  <overlay>
   <particlesystem>
  <condition>
   <greater-than>
    <property>fdm/jsbsim/hydro/vbx-fps</property>
    <value>7.0</value>
   </greater-than>
  </condition>
    <shooter>
     <phi-min-deg>-45.0</phi-min-deg>
     <phi-max-deg>-50.0</phi-max-deg>
    </shooter>
   </particlesystem>
  </overlay>
  <path>Aircraft/USS_Olympia/Models/Effects/wake.xml</path>
  <offsets>
   <!-- x/y/z == back/right/up -->
   <x-m>  0.00 </x-m>
   <y-m>  0.00 </y-m>
   <z-m>  0.00 </z-m>
  </offsets>
 </model>


 <model>
  <name>Bow Splash Starboard</name>
  <overlay>
   <particlesystem>
