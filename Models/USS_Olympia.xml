        <property>fdm/jsbsim/external_reactions/propeller/x</property> <!-- can be set to -1 via key binding 127-Delete to reverse engines -->
      </product>
    </expression>
    <center>
      <x-m> 100.955 </x-m>
      <y-m>  3.085 </y-m>
      <z-m> -4.099 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
